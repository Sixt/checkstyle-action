<?xml version="1.0"?>
<!DOCTYPE import-control PUBLIC "-//Checkstyle//DTD ImportControl Configuration 1.4//EN" "https://checkstyle.org/dtds/import_control_1_4.dtd">
<import-control pkg="com.sixt">
	<disallow pkg="io.micrometer" />
	<disallow pkg="org.springframework.lang" />
	<allow pkg=".*" regex="true" />
	<subpackage name="service">
		<disallow
			class="org.springframework.boot.autoconfigure.jdbc.DataSourceProperties" />
	</subpackage>
	<subpackage name="lib">
		<disallow
			class="org.springframework.boot.autoconfigure.jdbc.DataSourceProperties" />
	</subpackage>
</import-control>